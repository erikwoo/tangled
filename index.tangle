body
	h3= new Date()
		&
			font-size 100px
	&
		color steelblue
		text-shadow 1px 1px 1px #777

	:iced($)
		module.exports = require 'jquery'

	:iced(clock)
		$ = require '$'
		setInterval ->
			$('h3').text new Date()
		, 1000

	:iced
		console.time 'a'
		await setTimeout defer(), 1000
		console.timeEnd 'a'
		require 'clock'
		